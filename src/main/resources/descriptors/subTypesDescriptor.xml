<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>subTypesDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <imports>
    <import name="descriptors.deiis_types"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.CASDocuments</name>
      <description/>
      <supertypeName>uima.tcas.DocumentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>Question</name>
          <description>Wrapper type around Question. Stores the Question of the Question/Answer Input Model.</description>
          <rangeTypeName>edu.cmu.deiis.types.subTypes.Question</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>Answers</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.subTypes.Answers</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.Answers</name>
      <description/>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>AnswerList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.subTypes.Answer</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.Sentence</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>TokenList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.subTypes.Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>NGramList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.subTypes.NGramSets</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.Token</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Token</supertypeName>
      <features>
        <featureDescription>
          <name>TokenString</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.Question</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Question</supertypeName>
      <features>
        <featureDescription>
          <name>SentenceStructure</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.subTypes.Sentence</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>QuestionsString</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.Answer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.AnswerScore</supertypeName>
      <features>
        <featureDescription>
          <name>SentenceStructure</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.subTypes.Sentence</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>AnswerString</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>CorrectAnswer</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.subTypes.NGramSets</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.NGram</supertypeName>
      <features>
        <featureDescription>
          <name>NGramTokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
        <elementType>edu.cmu.deiis.types.subTypes.Token</elementType>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
